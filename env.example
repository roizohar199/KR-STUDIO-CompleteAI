# OpenAI API Key
# החלף את your_api_key_here עם ה-API Key האמיתי שלך מ-OpenAI
VITE_OPENAI_API_KEY=your_api_key_here

# הערה: 
# 1. העתק את הקובץ הזה ל-.env
# 2. החלף את your_api_key_here עם ה-API Key האמיתי שלך
# 3. קבל API Key מ: https://platform.openai.com/api-keys

# Worker Configuration
# אם ה-Worker רץ על שרת נפרד, הגדר את הכתובת החיצונית
# WORKER_URL=https://your-worker-service.onrender.com/api/worker

# אם ה-Worker רץ על אותו שרת, הגדר את הפורט
WORKER_PORT=10001

# Server Configuration
PORT=10000 